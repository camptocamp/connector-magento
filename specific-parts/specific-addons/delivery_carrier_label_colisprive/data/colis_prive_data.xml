<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="1">

      <!-- Add colis prive claim category -->
      <record model="crm.case.categ" id="categ_claim_colisprive_incident">
          <field name="name">Colis prive - Incident WS</field>
          <field name="section_id" ref="stockit_synchro.section_stockit_error"/>
          <field name="object_id" search="[('model','=','crm.claim')]" model="ir.model"/>
      </record>

      <!-- Add colis prive partner -->
      <record id="colis_prive_partner" model="res.partner">
          <field name="name">COLIS PRIVE</field>
          <field name="country_id" search="[('code','=','FR')]" model="res.country"/>
      </record>

      <!-- Add colis prive transport file. -->
      <record id="colisprive_pudo_delivery_file" model="delivery.carrier.file">
          <field name="name">Colisprive PUDO shipping</field>
          <field name="type">colisprive</field>
          <field name="auto_export" eval="True"/>
          <field name="write_mode">disk</field>
          <field name="export_path">/srv/openerp/instances</field>
          <field name="destype">PUDO</field>
          <field name="ispcl_withpod" eval="False"/>
      </record>

      <record id="colisprive_locker_delivery_file" model="delivery.carrier.file">
          <field name="name">Colisprive LOCKER shipping</field>
          <field name="type">colisprive</field>
          <field name="auto_export" eval="True"/>
          <field name="write_mode">disk</field>
          <field name="export_path">/srv/openerp/instances</field>
          <field name="destype">LOCKER</field>
          <field name="ispcl_withpod" eval="False"/>
      </record>

      <record id="colisprive_domicile_delivery_file" model="delivery.carrier.file">
          <field name="name">Colisprive DOMICILE shipping</field>
          <field name="type">colisprive</field>
          <field name="auto_export" eval="True"/>
          <field name="write_mode">disk</field>
          <field name="export_path">/srv/openerp/instances</field>
          <field name="ispcl_withpod" eval="False"/>
      </record>

      <record id="colisprive_domicile_pod_delivery_file" model="delivery.carrier.file">
          <field name="name">Colisprive DOMICILE with POD shipping</field>
          <field name="type">colisprive</field>
          <field name="auto_export" eval="True"/>
          <field name="write_mode">disk</field>
          <field name="export_path">/srv/openerp/instances</field>
          <field name="ispcl_withpod" eval="True"/>
      </record>

      <!-- Add colis prive transport mode. -->
      <record id="colisprive_pointrelais_delivery" model="delivery.carrier">
          <field name="name">Colisprive Pointrelais</field>
          <field name="partner_id" ref="colis_prive_partner"/>
          <field name="carrier_file_id" ref="colisprive_pudo_delivery_file"/>
          <field name="product_id" search="[('default_code', '=', 'SHIP MAGENTO')]" model="product.product"/>
          <field name="magento_code">colisprive_pointrelais</field>
          <field name="type">colisprive</field>
          <field name="ws_url">https://www.test.colisprive.com/Externe/WSCP.asmx?WSDL</field>
          <field name="ws_username">MONLOGIN</field>
          <field name="ws_password">MonMotDePasse</field>
          <field name="ws_customer_id">17</field>
          <field name="ws_account_id">210217</field>
      </record>

      <record id="colisprive_abricolis_delivery" model="delivery.carrier">
          <field name="name">Colisprive Abricolis</field>
          <field name="partner_id" ref="colis_prive_partner"/>
          <field name="carrier_file_id" ref="colisprive_locker_delivery_file"/>
          <field name="product_id" search="[('default_code', '=', 'SHIP MAGENTO')]" model="product.product"/>
          <field name="magento_code">colisprive_abricolis</field>
          <field name="type">colisprive</field>
          <field name="ws_url">https://www.test.colisprive.com/Externe/WSCP.asmx?WSDL</field>
          <field name="ws_username">MONLOGIN</field>
          <field name="ws_password">MonMotDePasse</field>
          <field name="ws_customer_id">17</field>
          <field name="ws_account_id">210217</field>
      </record>

      <record id="colisprive_domicile_delivery" model="delivery.carrier">
          <field name="name">Colisprive Domicile</field>
          <field name="partner_id" ref="colis_prive_partner"/>
          <field name="carrier_file_id" ref="colisprive_domicile_delivery_file"/>
          <field name="product_id" search="[('default_code', '=', 'SHIP MAGENTO')]" model="product.product"/>
          <field name="magento_code">colisprive_domicile</field>
          <field name="type">colisprive</field>
          <field name="ws_url">https://www.test.colisprive.com/Externe/WSCP.asmx?WSDL</field>
          <field name="ws_username">MONLOGIN</field>
          <field name="ws_password">MonMotDePasse</field>
          <field name="ws_customer_id">17</field>
          <field name="ws_account_id">210217</field>
      </record>

      <record id="colisprive_domicile_pod_delivery" model="delivery.carrier">
          <field name="name">Colisprive Domicile POD</field>
          <field name="partner_id" ref="colis_prive_partner"/>
          <field name="carrier_file_id" ref="colisprive_domicile_pod_delivery_file"/>
          <field name="product_id" search="[('default_code', '=', 'SHIP MAGENTO')]" model="product.product"/>
          <field name="magento_code">colisprive_domicile_POD</field>
          <field name="type">colisprive</field>
          <field name="ws_url">https://www.test.colisprive.com/Externe/WSCP.asmx?WSDL</field>
          <field name="ws_username">MONLOGIN</field>
          <field name="ws_password">MonMotDePasse</field>
          <field name="ws_customer_id">17</field>
          <field name="ws_account_id">210217</field>
      </record>

      <!--WS error Email template -->
      <record id="web_service_error_template_mail" model="email.template">
          <field name="name">Colis privé WS errors</field>
          <field name="email_from">contact@debonix.fr</field>
          <field name="email_recipients"></field>
          <field name="subject">Error with Colis Privé Web Service</field>
          <field name="email_to">info-dsi@debonix.fr</field>
          <field name="model_id" ref="crm_claim.model_crm_claim"/>
          <field name="auto_delete" eval="True"/>
          <field name="lang">${user.lang or 'fr_FR'}</field>
          <field name="body_html"><![CDATA[
<div style="font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; ">

    <p>Hello,</p>

    <p>${object.description or ''}</p>

</div>
]]></field>
      </record>


  </data>
</openerp>
