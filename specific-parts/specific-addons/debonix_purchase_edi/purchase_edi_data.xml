<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="ir_cron_edifact_check_removed" model="ir.cron">
            <field name="name">Check EDIFACT drop dir for processed purchase orders</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'purchase.order'" name="model"/>
            <field eval="'check_removed_edifact_files'" name="function"/>
            <field eval="'()'" name="args"/>
            <field eval="False" name="active"/>
        </record>

        <record id="ir_cron_edifact_import_tracking" model="ir.cron">
            <field name="name">Import tracking for SOGEDESCA orders</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'edi.import.tracking'" name="model"/>
            <field eval="'import_csv_tracking'" name="function"/>
            <field eval="'()'" name="args"/>
            <field eval="False" name="active"/>
        </record>

        <record model="crm.case.categ" id="categ_claim_pln_incident">
            <field name="name">PLN Incident</field>
            <field name="section_id"
                   ref="stockit_synchro.section_stockit_error"/>
            <field name="object_id" search="[('model','=','crm.claim')]" model="ir.model"/>
        </record>
    </data>
</openerp>
